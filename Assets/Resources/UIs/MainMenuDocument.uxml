    <ui:GroupBox name="storeScreenGroup" class="screenGroup" style="display: none;" />
    <ui:GroupBox name="toolsScreenGroup" class="screenGroup" style="display: none;" />
    <ui:GroupBox name="loreScreenGroup" class="screenGroup" style="display: none;" />
</ui:UXML>
