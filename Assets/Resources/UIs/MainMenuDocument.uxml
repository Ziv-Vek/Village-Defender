                <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="unitName" />
                <ui:VisualElement name="unitPriceBox" style="flex-grow: 1;">
                    <ui:VisualElement style="flex-grow: 1; background-image: resource(&apos;Sprites/coin&apos;); -unity-background-scale-mode: scale-to-fit;" />
