            <ui:Button text="Farm" parse-escape-sequences="true" display-tooltip-when-elided="true" name="progressionScreenButton" class="screens-container__button" />
            <ui:Button text="Home" parse-escape-sequences="true" display-tooltip-when-elided="true" name="homeScreenButton" class="screens-container__button" />
            <ui:Button text="Tools" parse-escape-sequences="true" display-tooltip-when-elided="true" name="powerupsScreenButton" class="screens-container__button" style="background-image: none;" />
            <ui:Button text="Lore" parse-escape-sequences="true" display-tooltip-when-elided="true" name="loreScreenButton" class="screens-container__button" />
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:GroupBox name="homeScreenGroup" class="screenGroup" style="display: none;">
        <ui:Button text="BATTLE" parse-escape-sequences="true" display-tooltip-when-elided="true" name="playButton" class="play-button" />
        <ui:Button parse-escape-sequences="true" display-tooltip-when-elided="true" name="settingButton" class="settings-button" />
    </ui:GroupBox>
    <ui:GroupBox name="farmScreenGroup" class="screenGroup">
        <ui:VisualElement class="farm-data-container" style="flex-grow: 1; opacity: 1;">
            <ui:VisualElement name="unitBox" class="farm-data-container__unit-box" style="flex-grow: 1;">
